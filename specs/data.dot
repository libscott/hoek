digraph G {
    // Transactions
    node [shape=hexagon]
    Fund
    Start
    Payout
    Dispute
    Dispute
    node [shape=oval]

    // Actors
    Player1
    Player2
    Dealer
    Notary

    edge [style=dashed]
    Player1 -> Fund
    Player2 -> Fund
    Dealer -> Start
    Dealer -> End
    Notary -> Payout
    edge [style=solid]

    Fund -> Start [label="txid"]

    Start -> End
    Fund -> Payout
    Player1 -> Payout
    Player2 -> Payout
    Dealer -> Payout

    edge [color=red, style=dashed]
    Dealer -> Dispute
    Player1 -> Dispute
    Player2 -> Dispute
    edge [style=solid]
    Start -> Dispute
    Dispute -> Notary

    

}

